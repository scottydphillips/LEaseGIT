<div class='container'>
    <div class='row gx-5 gy-3'>
        <div class="col-md-9">
            <h3>Property Details</h2>
            <p>Property ID: {{id}}</p>
            <p>Property Address: {{address}}</p>
            {{#if availability}}
                <p>Property Availability: available</p>
                {{else}}
                <p>Property Availability: unavailable</p>
            {{/if}}
        </div>
<hr>
        <div class='col-md-9'>
            <h3>Owner's Informations:</h3>
            <p>Name: {{user.name}}</p>
            <p>Email: {{user.email}}</p>
            <p>Phone#: {{user.phone}}</p>
        </div>
    </div>
</div>

